{
  "version": 2,
  "builds": [
    { "src": "src/api/webhook.ts", "use": "@vercel/node" },
    { "src": "src/api/setWebhook.ts", "use": "@vercel/node" },
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/webhook", "dest": "src/api/webhook.ts" },
    { "src": "/api/setWebhook", "dest": "src/api/setWebhook.ts" },
    {
      "src": "/(.*)",
      "dest": "/src/server.ts"
    }
  ]
} 