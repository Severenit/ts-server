# Telegram Bot Token
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Режим работы (true - webhook, false - long polling)
USE_WEBHOOK=false

# Окружение (development/production)
ENV=development

# Порт для локального сервера
PORT=3000

# URL для webhook (автоматически устанавливается Vercel)
# VERCEL_URL=https://your-app-url.vercel.app 